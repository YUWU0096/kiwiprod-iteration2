<!DOCTYPE html>
<html>
<body>

    <SCRIPT LANGUAGE="JavaScript">
        var testV = true;
        function password() {

            var pass1 = prompt('Please enter the password:', '');
            while (testV) {
                if (!pass1)
                    history.go(-1);
                if (pass1 == "kiwi") {
                    alert('correct');
                    testV = false;
                    window.location.href = "../Home/Homepage";
                    break;
                }

                var pass1 =
                    prompt('incorrect,please try again');
            }
            if (pass1 != "password" & testV == 999)
                history.go(-1);
            return " ";
        }
        document.write(password());
    </SCRIPT>
</body>

</html>